{
  "schemaVersion": 1,
  "id": "pear_expansion",
  "version": "${version}",
  "name": "Pear Expansion",
  "description": "Pear Expansion is a small Minecraft mod that adds various features, blocks, items, and tweaks â€” just things I felt like adding.",
  "authors": [
    "RobiPoire"
  ],
  "contact": {},
  "license": "GPL-3.0",
  "icon": "assets/pear_expansion/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      {
        "adapter": "kotlin",
        "value": "net.pearadise.pearexpansion.client.PearExpansionDataGenerator"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "net.pearadise.pearexpansion.client.PearExpansionClient"
      }
    ],
    "main": [
      {
        "adapter": "kotlin",
        "value": "net.pearadise.pearexpansion.PearExpansion"
      }
    ]
  },
  "mixins": [
    "pear_expansion.mixins.json",
    {
      "config": "pear_expansion.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "trinkets": ">=${trinkets_version}",
    "cardinal-components-base": ">=${cardinal_components_version}",
    "cardinal-components-entity": ">=${cardinal_components_version}",
    "minecraft": "${minecraft_version}"
  }
}
